      steps {
        echo 'Assalamu Aleikum'
      }
    }

    stage('Stage2') {
      steps {
        echo 'Stage2'
      }
    }

    stage('Stage3') {
      steps {
        echo 'Stage3'
      }
    }

    stage('Test') {
      steps {
        sh 'echo "Hello"'
      }
    }

  }
}